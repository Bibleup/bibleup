{
  "name": "bibleup",
  "version": "1.0.0-beta",
  "description": "A tool for converting bible references on a webpage to links with an accessible popup",
  "type": "module",
  "browser": "./dist/bibleup.min.js",
  "exports": {
    ".": {
      "import": "./dist/bibleup.esm.min.js",
      "require": "./dist/bibleup.min.js"
    },
    "./core": {
      "import": "./dist/bibleup-core.esm.min.js",
      "require": "./dist/bibleup-core.min.js"
    }
  },
  "files": [
    "dist", "demo"
  ],
  "scripts": {
    "build": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bibleup/bibleup.js.git"
  },
  "keywords": [
    "bible",
    "scripture",
    "tagger",
    "popup",
    "reference",
    "browser"
  ],
  "author": "BibleUp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bibleup/bibleup.js/issues"
  },
  "homepage": "https://github.com/Bibleup/bibleup.js#readme",
  "devDependencies": {
    "rollup": "^2.45.2",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
