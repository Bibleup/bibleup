    "url": "git+https://github.com/Bibleup/bibleup.js.git"
  },
  "keywords": [
    "bible",
    "scripture",
    "tagger",
    "popup",
    "reference",
    "browser"
  ],
  "author": "BibleUp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bibleup/bibleup.js/issues"
  },
  "homepage": "https://bibleup.netlify.app",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rollup": "^2.45.2",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
