{
  "name": "@bibleup/bibleup",
  "version": "1.0.1",
  "description": "A tool for converting Bible references on a webpage to links with an accessible popup",
  "type": "module",
  "browser": "./dist/umd/bibleup.min.js",
  "unpkg": "./dist/umd/bibleup.min.js",
  "module": "./dist/esm/bibleup.esm.js",
  "types": "./dist/bibleup.d.ts",
  "exports": {
    ".": "./dist/esm/bibleup.esm.js",
    "./css": "./dist/css/bibleup.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"No test specified\"",
    "build": "npm run lint && npm run clean && rollup --config",
    "lint": "eslint --fix --ext .js,.ts ./bibleup",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bibleup/bibleup.js.git"
  },
  "keywords": [
    "bible",
    "scripture",
    "tooltip",
    "popup",
    "references",
    "popover",
    "javascript"
  ],
  "author": "BibleUp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bibleup/bibleup.js/issues"
  },
  "homepage": "https://bibleup.netlify.app",
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "github": {
      "release": true
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/rollup-plugin-less": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "core-js": "^3.30.0",
    "del": "^6.1.1",
    "eslint": "^8.38.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "rimraf": "^4.4.1",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-license": "^2.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
